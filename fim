#!/bin/bash
#-----------------------------------------------------------------------------------------------------------------------
# Fim (File Integrity Manager)
#-----------------------------------------------------------------------------------------------------------------------

currentDir=`pwd`

baseDir=`dirname "$(readlink -f "$0")"`

JAVA_OPTIONS="-Xmx4096m"

java ${JAVA_OPTIONS} -jar ${baseDir}/target/fim-*.jar "${1}" "${2}" "${3}" "${4}" "${5}" "${6}"
